// Adjusts Koth round timers in minmode.

"Resource/UI/HudObjectiveKothTimePanel.res"
{	
	"BlueTimer"
	{
		"TimePanelValue"
		{
			if_match
			{
				"ypos_minmode"	"10"
				"font_minmode"	"HudFontSmallestBold"
			}
		}	
	}

	"RedTimer"
	{
		"TimePanelValue"
		{
			if_match
			{
				"ypos_minmode"	"10"
				"font_minmode"	"HudFontSmallestBold"
			}
		}	
	}
}
